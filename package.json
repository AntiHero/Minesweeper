		"ts-loader": "^9.2.6",
		"typescript": "^4.5.4",
		"webpack": "^5.65.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.7.1"
	},
	"lint-staged": {
		"*.{js,ts,tsx}": "eslint --cache --fix",
		"*.{js,ts,tsx,json,html,css,md}": "prettier --write"
	},
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	}
